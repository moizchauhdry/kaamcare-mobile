{
  "expo": {
    "name": "Kaam Care",
    "slug": "kaam-care",
    "version": "1.0.4",
    "owner": "nextbridgesystem",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "kaamcare",
    "splash": {
      "image": "./src/assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "experiments": {
      "tsconfigPaths": false
    },
    "plugins": [
      ["expo-av"],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your doctor."
        }
      ],
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow KaamCare to use Face ID/Fingerprnt. To authenticate and access your app."
        }
      ],
      "expo-secure-store",
      "expo-apple-authentication",
      "@react-native-google-signin/google-signin"
    ],
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "usesAppleSignIn": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": false,
      "bundleIdentifier": "com.qodeca.kaamcare",
      "buildNumber": "168",
      "entitlements": {
        "aps-environment": "development"
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "splash": {
        "backgroundColor": "#ffffff",
        "image": "./src/assets/splash.png"
      },
      "icon": "./src/assets/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon.png",
        "backgroundColor": "#0072EF"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ],
      "blockedPermissions": [
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO"
      ],
      "package": "com.qodeca.kaamcare"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c0a565e1-d631-4c78-8376-2bc72fa63568"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/c0a565e1-d631-4c78-8376-2bc72fa63568"
    }
  }
}
